@model IEnumerable<MvcMovieStore.Models.Genre>

<span><br /><br /><br /><br /><br /><br /></span>

        @foreach (var genre in Model)
    {
        @Html.ActionLink(genre.Name,
                        "Browse", "Movies",
                        new { Genre = genre.Name }, new { @class = "btn btn-block btn-default btn-md" })
}
